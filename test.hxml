--library utest
--class-path library
--class-path test\src
--main Main

--each
--library hxnodejs
-w -WDeprecatedEnumAbstract
--js test\bin\index.js
--cmd echo ============= js
--cmd cmd /c "cd test\bin & node index.js"

--next
--neko test\bin\index.n
--cmd echo ============= neko
